{
  "expo": {
    "name": "RnPlayground",
    "slug": "RnPlayground",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tecsinapse.RnPlayground"
    },
    "extra": {
      "eas": {
        "projectId": "edf0f9e6-e02f-464f-abfa-f532c69f0777"
      }
    },
    "owner": "tecsinapse",
    "plugins": [
      "expo-font"
    ]
  }
}
