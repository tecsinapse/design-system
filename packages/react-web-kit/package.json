{
  "name": "@tecsinapse/react-web-kit",
  "description": "TecSinapse React components",
  "version": "1.21.3",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "yarn build:es --watch",
    "build:es": "yarn build:cjs && yarn build:esm",
    "build:cjs": "cross-env BABEL_ENV=build babel src --root-mode upward --out-dir dist --source-maps --extensions .ts,.tsx --delete-dir-on-start --no-comments",
    "build:esm": "cross-env BABEL_ENV=build babel src --root-mode upward --out-dir esm --source-maps --extensions .ts,.tsx --delete-dir-on-start --no-comments --config-file ./babel.config.esm.js",
    "build:declaration": "yarn build:declaration:cjs && yarn build:declaration:esm",
    "build:declaration:cjs": "tsc --project tsconfig.build.json",
    "build:declaration:esm": "tsc --project tsconfig.build.esm.json",
    "build:declaration:watch": "tsc --project tsconfig.build.json --watch",
    "yalc:push": "yalc push --changed",
    "yalc:show": "yalc installations show $npm_package_name"
  },
  "dependencies": {
    "@emotion/native": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@tecsinapse/react-core": "^1.20.0",
    "@types/react-native": "^0.64.4",
    "react-native-vector-icons": "^9.2.0",
    "react-transition-group": "^4.4.2"
  },
  "repository": {
    "type": "git",
    "directory": "packages/react-web-kit",
    "url": "git+ssh://git@github.com/tecsinapse/design-system.git"
  },
  "bugs": {
    "url": "https://github.com/tecsinapse/design-system/issues"
  },
  "homepage": "https://tecsinapse.github.io/design-system/",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-native-web": "^0.17.1 || ^0.18.0"
  }
}
